{
  "name": "@bloxmove-com/did-asset-library-core-obfuscated",
  "version": "0.0.21",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/bloxmove-com/did-asset-library-obfuscated.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "scripts": {
    "docs": "typedoc --out docs src",
    "build": "npx hardhat compile && tsc",
    "build:obfuscated": "npm run build && javascript-obfuscator ./lib --output ./lib-obfuscated",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "lint:fix": "tslint -p tsconfig.json --fix",
    "deploy:ganache": "npx hardhat run --network ganache ./scripts/deploy-script.js",
    "deploy:hardhat": "npx hardhat run --network localhost ./scripts/deploy-script.js",
    "deploy:besuk3s": "npx hardhat run --network besuk3s ./scripts/deploy-script.js",
    "deploy:dev": "npx hardhat run --network dev ./scripts/deploy-script.js",
    "deploy:uat": "npx hardhat run --network uat ./scripts/deploy-script.js",
    "test:ganache": "jest --config jestconfig.json --forceExit --runInBand --verbose --web3Provider=ganache",
    "test:hardhat": "jest --config jestconfig.json --forceExit --runInBand --verbose --web3Provider=hardhat",
    "test:besuk3s": "jest --config jestconfig.json --forceExit --runInBand --verbose --web3Provider=besuk3s",
    "test:dev": "jest --config jestconfig.json --forceExit --runInBand --verbose --web3Provider=dev",
    "test:uat": "jest --config jestconfig.json --forceExit --runInBand --verbose --web3Provider=uat",
    "tsc:version": "tsc --version",
    "prepare": "cd .. && husky install core/.husky"
  },
  "keywords": [],
  "author": "bloXmove",
  "license": "ISC",
  "dependencies": {
    "@digitalbazaar/ed25519-signature-2018": "^2.0.1",
    "@digitalbazaar/ed25519-verification-key-2018": "^3.1.1",
    "@ensdomains/ensjs": "^2.0.1",
    "@ethersproject/experimental": "^5.5.0",
    "@transmute/json-web-signature-2020": "^2.0.0",
    "base64url": "^3.0.1",
    "bs58": "^4.0.1",
    "credentials-context": "^2.0.0",
    "did-context": "^3.1.1",
    "did-jwt": "5.8.0",
    "did-resolver": "^3.1.2",
    "dotenv": "^8.2.0",
    "ed25519-signature-2018-context": "^1.1.0",
    "ethers": "^5.5.1",
    "ethr-did": "2.1.4",
    "ethr-did-resolver": "^4.3.4",
    "husky": "^7.0.4",
    "ipfs-http-client": "^52.0.5",
    "js-base64": "^3.4.5",
    "jsonld-signatures": "^9.3.0",
    "ontology-ts-sdk": "^1.1.9",
    "uuid": "^8.3.2",
    "vc-js": "^0.6.4",
    "web-did-resolver": "^2.0.6",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@ensdomains/ens-contracts": "0.0.7",
    "@nomiclabs/hardhat-ethers": "^2.0.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "4.3.2",
    "@types/jest": "^24.9.1",
    "@types/uuid": "^8.3.1",
    "@types/zen-observable": "^0.8.3",
    "chai": "^4.3.4",
    "codecov": "^3.6.1",
    "hardhat": "^2.6.4",
    "javascript-obfuscator": "^4.0.0",
    "jest": "^24.3.0",
    "prettier": "^2.0.0",
    "solc": "^0.8.7-fixed",
    "truffle-assertions": "^0.9.2",
    "ts-jest": "^24.3.0",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.0",
    "typedoc": "^0.19.0",
    "typescript": "4.0.3"
  },
  "files": [
    "lib/**/*",
    "artifacts/**/*"
  ]
}
